<article class="container">
  <h1><%= page.title %></h1>
  <div class="meta">
    <time><%- date(page.date) %></time>
    <% page.tags.each(function(tag) { %>
    <a class="tag-link" href="/<%- tag.path %>"><%- tag.name %></a>
    <% }) %>
  </div>
  <div class="article-cnt">
    <%- page.content %>
  </div>
</article>
